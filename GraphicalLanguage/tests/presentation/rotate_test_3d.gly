mode: 3D

canvas: 600, 600, #black

-- X, Y, Z AXIS
vector X0: 0,0,0
vector X1: 15,0,0
vector Y0: 0,0,0
vector Y1: 0,15,0
vector Z0: 0,0,0
vector Z1: 0,0,15

axis X: X0, X1
axis Y: Y0, Y1
axis Z: Z0, Z1

fill X: #red
fill Y: #green
fill Z: #blue

draw X
draw Y
draw Z


-- VECTOR
vector Vector1: 0, 0, 0
vector Vector2: 0, 5, 0


-- BOX
vector BoxInit1: 5, 5, 0
vector BoxSize: 1, 2, 3
box Box: BoxInit1, BoxSize


-- SPHERE
vector SphereInit1: 0, 0, 0
vector SphereInit2: 0, 0, 0
sphere Sphere1: SphereInit1, 2
sphere Sphere2: SphereInit2, 2


-- CURVE
vector V0: 0, 0, 0
vector V1: 0, 0, -5
vector V2: 0, 10 ,-5
list<vectors> Vectors: V0, V1, V2
curve Curve: Vectors


-- AXIS
vector AxisInit: 2, 0, 0
vector AxisTerm: 2, 4, 0
axis Axis: AxisInit, AxisTerm



-- CYLINDER
vector CylinderInit: 6, 6, 0
vector CylinderTerm: 9, 9, 0
cylinder Cylinder: CylinderInit, CylinderTerm, 1


-- RING
vector RingInit: 3, -3, 3
vector RingTerm: 5, -5, 5
ring Ring: RingInit, RingTerm, 1, 0.3






rotate Cylinder: 181, Axis

draw Cylinder

rotate Cylinder: 33, Axis

draw Cylinder

rotate Cylinder: 33, Axis

draw Cylinder

rotate Cylinder: 33, Axis

draw Cylinder

rotate Cylinder: 33, Axis

draw Cylinder

rotate Cylinder: 33, Axis

draw Cylinder

rotate Cylinder: 33, Axis

draw Cylinder

rotate Cylinder: 33, Axis

draw Cylinder

rotate Cylinder: 33, Axis

draw Cylinder

rotate Cylinder: 33, Axis

draw Cylinder

rotate Cylinder: 180, Axis

draw Cylinder





